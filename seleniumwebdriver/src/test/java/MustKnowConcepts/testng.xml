<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite thread-count="5" name="Suite" parallel= "tests">
	
	<listeners>
		<listener class-name="MustKnowConcepts.MyListener1" />
	</listeners>
	
	  <test name="ChromeParallelTest">
	    
	    <parameter name = "browser" value = "chrome"/>
	    <parameter name="URL" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
	    <classes>
	      <class name="MustKnowConcepts.ParmTest"/>
	    </classes>
	  </test> <!-- ParallelTest -->
  
  
  <test name="EdgeParallelTest">
    
    <parameter name = "browser" value = "Edge"/>
    <parameter name="URL" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
    <classes>
      <class name="MustKnowConcepts.ParmTest"/>
    </classes>
  </test> <!-- ParallelTest -->
  
  
  <test name="FirefoxParallelTest">
    
    <parameter name = "browser" value = "firefox"/>
    <parameter name="URL" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
    <classes>
      <class name="MustKnowConcepts.ParmTest"/>
    </classes>
  </test> <!-- ParallelTest -->
  
  
  
</suite> <!-- Suite -->

